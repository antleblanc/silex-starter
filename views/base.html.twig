<!DOCTYPE HTML>
    <html lang="fr-FR">
    <head>
        <meta charset="utf-8">
        <title>{% block title %}PROJECT_NAME{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% for flashMessage in app.session.flashbag.get('notice') %}
        <div class="flash-notice">
            {{ flashMessage }}
        </div>
        {% endfor %}
        {% block content %}{% endblock %}
        {% block scripts %}{% endblock %}
    </body>
</html>
